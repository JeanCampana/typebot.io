# Typebot Environment Configuration

# Database
DATABASE_URL="postgresql://postgres:typebot@typebot-db:5432/typebot"

# Encryption
ENCRYPTION_SECRET="your-32-character-secret-key-here"

# NextAuth
NEXTAUTH_URL="http://localhost:8080"
NEXTAUTH_SECRET="your-nextauth-secret-key-here"

# Viewer URL
NEXT_PUBLIC_VIEWER_URL="http://localhost:8081"

# Admin Configuration
ADMIN_EMAIL="admin@example.com"
DEFAULT_WORKSPACE_PLAN="FREE"

# Redis
REDIS_URL="redis://typebot-redis:6379"

# SMTP (optional - for email notifications)
# SMTP_HOST=""
# SMTP_USERNAME=""
# SMTP_PASSWORD=""
# SMTP_PORT="587"
# SMTP_SECURE="true"
# NEXT_PUBLIC_SMTP_FROM=""

# S3 Storage (optional - for file uploads)
# S3_ACCESS_KEY=""
# S3_SECRET_KEY=""
# S3_BUCKET="typebot"
# S3_REGION=""
# S3_ENDPOINT=""

# Debug
DEBUG="false"
DISABLE_SIGNUP="false" 